            "caption": "Variablen wirklich automatisch ermitteln?",
            "items": [
              {
                "type": "Button",
                "caption": "Automatisch ermitteln",
                "onClick": "SABO_DetermineVariables($id);"
              }
            ]
          }
        },
        {
          "type": "PopupButton",
          "caption": "Variablenprofile zuweisen",
          "popup": {
            "caption": "Variablenprofile wirklich zuweisen?",
            "items": [
              {
                "type": "Button",
                "caption": "Variablenprofile zuweisen",
                "onClick": "SABO_AssignVariableProfile($id, true);"
              }
            ]
          }
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "Anzeigeoptionen",
      "items": [
        {
          "type": "Button",
