            {
              "name": "Address",
              "caption": "Addresse",
              "width": "250px",
              "add": "",
              "edit": {
                "type": "ValidationTextBox"
              }
            }
          ]
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "Anzeigeoptionen",
